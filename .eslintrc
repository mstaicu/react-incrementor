{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-param-reassign": 0,
    "camelcase": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 0,
    "import/prefer-default-export": 0,
    "no-unused-expressions": 0
  }
}
